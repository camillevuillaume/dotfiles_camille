    {
        "type": "shell",
        "key": "│ ├",
        "keyColor": "#bb9af7"
    },
    {
        "type": "lm",
        "key": "│ ├",
        "keyColor": "#bb9af7"
    },
    {
        "type": "wm",
        "key": "│ ├",
        "keyColor": "#bb9af7"
    },
    {
        "type": "wmtheme",
        "key": "│ ├󰉼",
        "keyColor": "#bb9af7"
    },
    {
        "type": "terminal",
        "key": "└ └",
        "keyColor": "#bb9af7"
    },
    {
        "type": "custom",
        "format": "\u001b[90m└────────────────────────────────────────────┘"
    },
    "break",
    {
        "type": "custom",
        "format": "\u001b[90m┌─────────────Uptime / Age / DT──────────────┐"
    },
    {
        "type": "command",
        "key": "  OS Age ",
        "keyColor": "#7aa2f7",
        "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days"
    },
    {
        "type": "uptime",
        "key": "  Uptime ",
        "keyColor": "#7aa2f7"
    },
    {
        "type": "datetime",
        "key": "  DateTime ",
        "keyColor": "#7aa2f7"
    },
    {
        "type": "custom",
        "format": "\u001b[90m└────────────────────────────────────────────┘"
    }

