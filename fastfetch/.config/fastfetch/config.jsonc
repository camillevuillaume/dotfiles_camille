        "keyColor": "#9ece6a"
    },
    {
        "type": "custom",
        "format": "\u001b[90m└────────────────────────────────────────────┘"
    },
    "break",
    {
        "type": "custom",
        "format": "\u001b[90m┌──────────────────Software──────────────────┐"
    },
    {
        "type": "os",
        "key": " OS",
        "keyColor": "#bb9af7"
    },
    {
        "type": "kernel",
        "key": "│ ├",
        "keyColor": "#bb9af7"
    },
    {
        "type": "bios",
        "key": "│ ├",
        "keyColor": "#bb9af7"
    },
